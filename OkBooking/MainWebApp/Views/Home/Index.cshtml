@{
    ViewBag.Title = "One Click Booking";
}

<div class="cont">
	<div class="window">
		<div class="view">
			@Html.Partial("_Login")
		</div>

		<nav class="menu">
			<div class="menu-content">
				<ul>
					<li>Home</li>
					<li>Find a Room</li>
					<li>Favorite Offices</li>
					<li>Favorite Rooms</li>
					<li>Active Reservations</li>
					<li>Settings</li>
					<li>FAQ</li>
					<li>About us</li>
					<li>Log out</li>
				</ul>
			</div>
		</nav>
	</div>
</div>

<ul>
	@foreach (System.Collections.DictionaryEntry ev in Environment.GetEnvironmentVariables()) {
		<li><strong>@ev.Key.ToString()</strong> - @ev.Value.ToString()</li>
	}
</ul>

<ul>
	@foreach (System.Configuration.ConnectionStringSettings cs in System.Configuration.ConfigurationManager.ConnectionStrings)
	{
		<li><strong>@cs.Name</strong> - @cs.ConnectionString</li>
	}
</ul>

<ul>
	@foreach (System.Collections.DictionaryEntry ev in Environment.GetEnvironmentVariables(EnvironmentVariableTarget.Machine)) {
		<li><strong>@ev.Key.ToString()</strong> - @ev.Value.ToString()</li>
	}
</ul>

<ul>
	@foreach (System.Collections.DictionaryEntry ev in Environment.GetEnvironmentVariables(EnvironmentVariableTarget.User))
	{
		<li><strong>@ev.Key.ToString()</strong> - @ev.Value.ToString()</li>
	}
</ul>

<ul>
	@foreach (System.Collections.DictionaryEntry ev in Environment.GetEnvironmentVariables(EnvironmentVariableTarget.Process)) {
		<li><strong>@ev.Key.ToString()</strong> - @ev.Value.ToString()</li>
	}
</ul>